{
	"name": "c12lan",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"lint": "pnpm run format && pnpm run lint:es && pnpm run lint:style",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint:es": "eslint . --fix",
		"lint:style": "stylelint './src/**/*.{css,scss,svelte}' --fix --ignore-path .gitignore",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-vercel": "^3.0.1",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-xo": "^0.43.1",
		"eslint-plugin-svelte": "^2.26.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.2.2",
		"postcss-html": "^1.5.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"sass": "^1.62.1",
		"stylelint": "^15.6.2",
		"stylelint-config-clean-order": "^5.0.1",
		"stylelint-config-standard-scss": "^9.0.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0"
	},
	"type": "module",
	"dependencies": {
		"@notionhq/client": "^2.2.5",
		"@sveltestack/svelte-query": "^1.6.0",
		"@svelteuidev/composables": "^0.12.1",
		"@svelteuidev/core": "^0.12.1",
		"@svelteuidev/motion": "^0.12.1",
		"axios": "^1.4.0",
		"dayjs": "^1.11.7"
	}
}
